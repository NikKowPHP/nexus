{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm install --legacy-peer-deps && next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "test:unit": "vitest tests/unit",
    "test:e2e": "playwright test",
    "test:install": "playwright install",
    "test": "npm run test:sequential",
    "test:parallel": "concurrently -k \"npm run test:unit\" \"npm run test:e2e\"",
    "test:sequential": "npm run test:unit && npm run test:e2e",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@sentry/nextjs": "^9.28.1",
    "@storybook/react": "^8.6.14",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^2.0.0",
    "@supabase/supabase-js": "^2.50.0",
    "autoprefixer": "^10.4.21",
    "mammoth": "^1.9.1",
    "next": "15.3.3",
    "pdf-parse": "^1.1.1",
    "rate-limiter-flexible": "^7.1.1",
    "react": "^19.0.0",
    "react-circular-progressbar": "^2.2.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.42.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/experimental-nextjs-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0-beta.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^3.2.3",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-storybook": "^9.0.8",
    "jsdom": "^26.1.0",
    "playwright": "^1.42.1",
    "react-router-dom": "^7.6.2",
    "storybook": "^9.0.8",
    "tailwindcss": "^4.1.10",
    "typescript": "^5",
    "vitest": "^3.2.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
